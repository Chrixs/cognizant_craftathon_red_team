        <th>Category</th>
      </tr>
      <% if !@transactions.empty? %>
        <% @transactions.each do |f| %>
          <tr align="center">
            <td>
              <%= f.company %>
            </td>
            <td>
              <%= 'Â£' + f.amount.to_s %>
            </td>
